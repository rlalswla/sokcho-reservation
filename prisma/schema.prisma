generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
}

model User {
  id           String        @id @default(cuid())
  name         String        @unique // 이제 이름이 유일한 식별자가 됩니다.
  reservations Reservation[]
}

model Reservation {
  id        String   @id @default(cuid())
  startDate DateTime
  endDate   DateTime
  memo      String?
  userId    String
  user      User     @relation(fields: [userId], references: [id])
}